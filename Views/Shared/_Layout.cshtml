<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"] - Dexa</title>

    <!-- Global stylesheets -->
    <link href="~/Assets/Global/css/icons/icomoon/styles.css" rel="stylesheet" type="text/css">
    <link href="~/Assets/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/Assets/css/bootstrap_limitless.min.css" rel="stylesheet" type="text/css">
    <link href="~/Assets/css/layout.min.css" rel="stylesheet" type="text/css">
    <link href="~/Assets/css/components.min.css" rel="stylesheet" type="text/css">
    <link href="~/Assets/css/colors.min.css" rel="stylesheet" type="text/css">
    <link href="~/Assets/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Assets/css/buttons.dataTables.min.css" rel="stylesheet" />

    <!-- /global stylesheets -->
    <!-- Core JS files -->
    <script src="~/Assets/Global/js/main/jquery.min.js"></script>
    <script src="~/Assets/Global/js/main/bootstrap.bundle.min.js"></script>
    <script src="~/Assets/Global/js/plugins/loaders/blockui.min.js"></script>
    <script src="~/Assets/Global/js/plugins/ui/ripple.min.js"></script>
    <!-- /core JS files -->
    <!-- DatatableTheme JS files -->
    <script src="~/Assets/Global/js/plugins/tables/datatables/datatables.min.js"></script>
    <script src="~/Assets/Global/js/plugins/tables/datatables/extensions/responsive.min.js"></script>
    <script src="~/Assets/Global/js/plugins/forms/selects/select2.min.js"></script>

    <script src="~/Assets/js/dataTables.buttons.min.js"></script>
    <script src="~/Assets/js/buttons.colVis.min.js"></script>
    <script src="~/Assets/js/buttons.flash.min.js"></script>
    <script src="~/Assets/js/jszip.min.js"></script>
    <script src="~/Assets/js/pdfmake.min.js"></script>
    <script src="~/Assets/js/vfs_fonts.js"></script>
    <script src="~/Assets/js/buttons.html5.min.js"></script>
    <script src="~/Assets/js/buttons.print.min.js"></script>
    <script src="~/Assets/Global/js/plugins/forms/selects/select2.min.js"></script>


    <script src="~/Assets/Global/js/plugins/ui/moment/moment.min.js"></script>
    <script src="~/Assets/Global/js/plugins/pickers/daterangepicker.js"></script>
    <script src="~/Assets/Global/js/plugins/pickers/anytime.min.js"></script>
    <script src="~/Assets/Global/js/plugins/pickers/pickadate/picker.js"></script>
    <script src="~/Assets/Global/js/plugins/pickers/pickadate/picker.date.js"></script>
    <script src="~/Assets/Global/js/plugins/pickers/pickadate/picker.time.js"></script>

    <script src="~/Assets/Global/js/plugins/uploaders/dropzone.min.js"></script>


    <script src="~/Assets/js/app.js"></script>
    <script src="~/Assets/Global/js/demo_pages/dashboard.js"></script>
    <script src="~/Assets/Global/js/demo_pages/datatables_responsive.js"></script>
    <script src="~/Assets/Global/js/demo_pages/components_buttons.js"></script>
    <script src="~/Assets/Global/js/demo_pages/form_select2.js"></script>
    <script src="~/Assets/Global/js/demo_pages/picker_date.js"></script>
    <script src="~/Assets/Global/js/demo_pages/form_validation.js"></script>
    <!-- /theme JS files -->
</head>

<body>

    <!-- Main navbar -->
    <div class="navbar navbar-expand-md navbar-dark bg-indigo navbar-static">
        <div class="navbar-brand">
            <a asp-controller="Home" asp-action="Index" class="d-inline-block">
                <img src="~/Assets/Global/images/logo_light.png" alt="">
            </a>
        </div>

        <div class="d-md-none">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar-mobile">
                <i class="icon-tree5"></i>
            </button>
        </div>

        <div class="collapse navbar-collapse" id="navbar-mobile">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a asp-controller="Home" asp-action="Index" class="navbar-nav-link">
                        <i class="icon-home mr-2"></i>
                        Dashboard
                    </a>
                </li>
                <li class="nav-item">
                    <a asp-controller="Outlet" asp-action="Index" class="navbar-nav-link">
                        <i class="icon-location4 mr-2"></i>
                        Outlet
                    </a>
                </li>
                <li class="nav-item">
                    <a asp-controller="Customer" asp-action="Index" class="navbar-nav-link">
                        <i class="icon-stack2 mr-2"></i>
                        Customer
                    </a>
                </li>
            </ul>

            <partial name="_LoginPartial" />
        </div>
    </div>
    <!-- /main navbar -->
    <!-- Page content -->
    <div class="page-content">

        <!-- Main content -->
        <div class="content-wrapper">
            <!-- Content area -->
            <div class="content">

                <partial name="_CookieConsentPartial" />

                @RenderBody()

            </div>
            <!-- /content area -->
            <!-- Footer -->
            <div class="navbar navbar-expand-lg navbar-light">
                <div class="text-center d-lg-none w-100">
                    <button type="button" class="navbar-toggler dropdown-toggle" data-toggle="collapse" data-target="#navbar-footer">
                        <i class="icon-unfold mr-2"></i>
                        Footer
                    </button>
                </div>

                <div class="navbar-collapse collapse" id="navbar-footer">
                    <span class="navbar-text">
                        &copy; 2020. <a href="#">Dexa</a> by <a href="#" target="_blank">IT Developement</a>
                    </span>
                </div>
            </div>
            <!-- /footer -->

        </div>
        <!-- /main content -->

    </div>
    <!-- /page content -->
    @RenderSection("Scripts", required: false)
</body>
</html>
